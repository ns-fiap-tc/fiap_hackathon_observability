{
  "dashboard": {
    "id": null,
    "title": "OpenTelemetry Collector - Monitoramento",
    "tags": ["opentelemetry", "collector", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Métricas Recebidas por Segundo",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(otelcol_receiver_accepted_metric_points[5m])",
            "legendFormat": "{{receiver}} - {{transport}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Métricas/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false
        }
      },
      {
        "id": 2,
        "title": "Métricas Exportadas por Segundo",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(otelcol_exporter_sent_metric_points[5m])",
            "legendFormat": "{{exporter}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Métricas/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false
        }
      },
      {
        "id": 3,
        "title": "Uso de Memória do Collector",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memória Residente",
            "refId": "A"
          },
          {
            "expr": "process_virtual_memory_bytes",
            "legendFormat": "Memória Virtual",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false
        }
      },
      {
        "id": 4,
        "title": "CPU Usage do Collector",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %",
            "min": 0,
            "max": 100
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false
        }
      },
      {
        "id": 5,
        "title": "Erros de Processamento",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(otelcol_receiver_refused_metric_points[5m])",
            "legendFormat": "Métricas Recusadas - {{receiver}}",
            "refId": "A"
          },
          {
            "expr": "rate(otelcol_exporter_send_failed_metric_points[5m])",
            "legendFormat": "Falhas de Envio - {{exporter}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Erros/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": false,
          "current": false,
          "max": false,
          "min": false
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 0
  }
}
